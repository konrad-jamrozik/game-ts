{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=11.4.2",
    "node": ">=24.3.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:cached": "eslint . --cache --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "format": "prettier . --check --end-of-line auto",
    "format:debug": "prettier . --check --end-of-line auto --log-level debug",
    "format:fix": "prettier . --end-of-line auto --write",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui --api.port=6174",
    "test:cov": "vitest --coverage",
    "check": "npm run format && npm run lint:cached && npm run test"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.8.0",
    "@mui/x-data-grid": "^8.8.0",
    "@reduxjs/toolkit": "^2.8.2",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "radash": "^12.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "redux-undo": "^1.1.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.6.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "fake-indexeddb": "^6.0.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
