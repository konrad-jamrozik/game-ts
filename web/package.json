{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=11.4.2",
    "node": ">=24.3.0"
  },
  "scripts": {
    "dev": "vite --no-open",
    "tsc": "tsgo --build",
    "vite_build": "vite build",
    "build": "npm run tsc && vite build",
    "oxlint": "oxlint . --type-aware --max-warnings 0",
    "eslint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:cached": "eslint . --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "format": "prettier . --check --end-of-line auto",
    "format:debug": "prettier . --check --end-of-line auto --log-level debug",
    "format:fix": "prettier . --end-of-line auto --write",
    "test": "vitest --run --project unit --project component",
    "test:unit": "vitest --run --project unit",
    "test:component": "vitest --run --project component",
    "test:e2e": "vitest --run --project e2e",
    "test:all": "vitest --run --project unit --project component --project e2e",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui --api.port=6174 --no-open",
    "test:cov": "vitest --coverage",
    "test:uicov": "vitest --ui --coverage.enabled=true",
    "depcruise": "depcruise src test --config .dependency-cruiser.js",
    "qcheck": "npm run oxlint && npm run depcruise && npm run tsc && npm run test",
    "check": "npm run format && npm run oxlint && npm run eslint:cached && npm run depcruise && npm run build && npm run test:all"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-charts": "^8.18.0",
    "@mui/x-data-grid": "^8.18.0",
    "@mui/x-tree-view": "^8.18.0",
    "@reduxjs/toolkit": "^2.10.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "pluralize": "^8.0.0",
    "radash": "^12.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "redux-undo": "^1.1.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "latest",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/eslint-plugin": "^1.4.3",
    "@vitest/ui": "^4.0.10",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "dependency-cruiser": "^17.3.5",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-oxlint": "^1.29.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-testing-library": "^7.13.4",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "oxlint": "^1.29.0",
    "oxlint-tsgolint": "^0.7.1",
    "prettier": "3.6.2",
    "tsgolint": "^0.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  }
}
